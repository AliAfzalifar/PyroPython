&MATL ID = 'LIGNIN'
	EMISSIVITY 		= 0.8
	DENSITY 		= 600.
	CONDUCTIVITY_RAMP	= 'RAMP_Birch_K', 
	SPECIFIC_HEAT_RAMP	= 'RAMP_Birch_C',
	N_REACTIONS		= 1
	A			= 48386259739.0
	E 			= 158625.549233 
