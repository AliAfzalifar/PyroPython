	EMISSIVITY 		= 1
	DENSITY 		= 600.
	CONDUCTIVITY_RAMP	= 'RAMP_Birch_K', 
	SPECIFIC_HEAT_RAMP	= 'RAMP_Birch_C',
	N_REACTIONS		= 1
	A			= 669218286834.0
	E 			= 171664.173096 
	N_S			= 0.485164085225 
	NU_SPEC		= 0.968419715506
	NU_MATL		= 0.0315802844937
	SPEC_ID		= 'PROPANE'
	MATL_ID		= 'CHAR'
	HEAT_OF_REACTION	 = 100
	HEAT_OF_COMBUSTION = 13500/

&MATL ID = 'HEMISELLU'
	EMISSIVITY 		= 1.0
	DENSITY 			= 600.
	CONDUCTIVITY_RAMP	= 'RAMP_Birch_K', 
	SPECIFIC_HEAT_RAMP	= 'RAMP_Birch_C',
	N_REACTIONS		= 1
	A			= 207504684484.0
	E 			= 147415.524549 
